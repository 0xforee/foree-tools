#!/bin/bash
### Description: mm的加强版工具
### Author: foree
### Date: 20151218

SOFT_DIR=$(dirname $(realpath $0))
source $SOFT_DIR/common.sh

mm 2>&1 |tee _push_tmp
PUSH_FILE=$(grep "^Install" _push_tmp |awk '{print $2}')
if [ -z "$PUSH_FILE" ];then
    add_color_for_echo "Not Found File To Push"
else
    for i in $PUSH_FILE
    do
        echo "Pushing $i"
        fadb push $i
    done
fi
rm _push_tmp
