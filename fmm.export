#!/bin/bash
### Description: mm的加强版工具
### Author: foree
### Date: 20151218

SOFT_DIR=$(dirname $(realpath $0))
source $SOFT_DIR/common.sh

mm 2>&1 |tee _push_tmp
PUSH_FILE=$(grep "^Install" _push_tmp |awk '{print $2}')
if [ -z "$PUSH_FILE" ];then
    flog -I "Not Found File To Push"
    flog -D "Not found"
else
    for i in $PUSH_FILE
    do
        flog -I "Pushing $i"
        fadb push $i
    done
fi
rm _push_tmp
